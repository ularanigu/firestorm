{
    "name": "ularanigu/firestorm",
    "description": "A complete browscap polyfill for php.",
    "keywords": [
        "php7",
        "browscap-polyfill",
        "browscap-ini",
        "browscap"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.0"
    },
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish6657@gmail.com",
            "homepage": "https://github.com/ularanigu"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ularanigu\\Firestorm\\": "./src"
        },
        "files": [
            "./lib/runtime.php",
            "./lib/functions.php",
            "./lib/polyfill.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6|^7",
        "vimeo/psalm": "^1"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
